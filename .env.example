# Database Configuration (Neon DB)
DATABASE_URL="postgresql+asyncpg://user:password@host:port/database_name"

# Redis Configuration (Upstash Redis)
REDIS_URL="rediss://default:<password>@<host>:<port>"

# Azure Celery Configuration (Azure Service Bus)
# For Azure Service Bus, you might need a connection string.
# Example: Endpoint=sb://<your-namespace>.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=<your-key>
AZURE_SERVICE_BUS_CONNECTION_STRING="Endpoint=sb://<your-namespace>.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=<your-key>"
